<p id="notice"><%= notice %></p>

<h1>Members</h1>

<table>
  <thead>
    <tr>
      <th>Position</th>
      <th>Name</th>
      <th>Id number</th>
      <th>Date initial</th>
      <th>Date end</th>
      <th>Email</th>
      <th>Company</th>
      <th>State</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @members.each do |member| %>
      <tr>
        <td><%= member.position.name %></td>
        <td><%= member.name %></td>
        <td><%= member.id_number %></td>
        <td><%= member.date_initial %></td>
        <td><%= member.date_end %></td>
        <td><%= member.email %></td>
        <td><%= member.company.name %></td>
        <td><%= member.state.name %></td>
        <td><%= link_to 'Show', member %></td>
        <%if signed_in? %>
        <td><%= link_to 'Edit', edit_member_path(member) %></td>
        <td><%= link_to 'Destroy', member, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        <%end%>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Member', new_member_path %>
