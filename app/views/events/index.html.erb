<p id="notice"><%= notice %></p>

<h1>Events</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>State</th>
      <th>Date initial</th>
      <th>Date end</th>
      <th>Hour initial</th>
      <th>Hour end</th>
      <th>Venue</th>
      <th>Value</th>
      <th>Organizer</th>
      <th>Company</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= event.name %></td>
        <td><%= event.description %></td>
        <td><%= event.state.name %></td>
        <td><%= event.date_initial %></td>
        <td><%= event.date_end %></td>
        <td><%= event.hour_initial %></td>
        <td><%= event.hour_end %></td>
        <td><%= event.venue.name %></td>
        <td><%= event.value %></td>
        <td><%= event.organizer %></td>
        <td><%= event.company.name %></td>
        <td><%= link_to 'Show', event %></td>
        <%if signed_in? %>
        <td><%= link_to 'Edit', edit_event_path(event) %></td>
        <td><%= link_to 'Destroy', event, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        <%end%>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Event', new_event_path %>
